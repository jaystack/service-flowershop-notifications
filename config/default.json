{
  "rascal": {
    "vhosts": {
      "flowershop": {
        "connection": {
          "slashes": true,
          "protocol": "amqp",
          "hostname": "localhost",
          "user": "guest",
          "password": "guest",
          "port": 5672,
          "vhost": "flowershop",
          "options": {
            "heartbeat": 5
          }
        },
        "queues": {
          "sendEmailMQ": {
            "options": {
              "arguments": {
                "x-message-ttl": 60000,
                "x-max-length": 5000
              }
            }
          }
        },
        "subscriptions": {
          "sendEmailMQ": {
            "queue": "sendEmailMQ"
          }
        }
      }
    }
  },
  "defer": 1000,
  "queuename": "sendEmailMQ",
  "mongodb": {
    "uri": "mongodb://localhost/flowershop"
  }
}